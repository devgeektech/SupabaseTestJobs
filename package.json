{
  "name": "ai-coach-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --ext js --exec node src/index.js",
    "lint:micro-actions": "node scripts/lint-micro-actions.js",
    "test": "echo \"No tests yet\" && exit 0",
    "build": "echo \"Nothing to build for Node server\"",
    "start:doppler": "doppler run -- npm start",
    "dev:doppler": "doppler run -- npm run dev",
    "build:doppler": "doppler run -- npm run build",
    "test:doppler": "doppler run -- npm test -- --ci"
  },
  "dependencies": {
    "@sentry/node": "^8.9.0",
    "@supabase/supabase-js": "^2.45.4",
    "csv-parse": "^5.5.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.6.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
